														<td><%=subject_info.subject_professor_name%></td>
														<th scope="row" class="even">학점/강의</th>
														<td><%=subject_info.subject_point%></td>
													</tr>
													<tr>
														<th scope="row" class="even">강의시간</th>
														<td><%=subject_info.subject_time%></td>
